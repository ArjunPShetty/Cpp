    printf("Enter the email :");
    scanf("%s",email);
    if(email[0]>='0'&&email[0]<='9')
    {
        printf("Invaild");
        return 0;
    }
    int count1=0,count2=0,count3=0;
    for(int i=0;i<strlen(email);i++)
    {
        if(email[i]>=65&&email[i]<=90)
        continue;
        else if(email[i]>=97&&email[i]<=122)
        continue;
        else if(email[i]>=48&&email[i]<=57)
        continue;
        else if(email[i]=='@')
        count1++;
        else if(email[i]='.')
        count2++;
        else
        count3++;
    }
    if(count1!=1||count2!=1||count3>0)
    {
        printf("Invaild");
        return 0;
    }
    char data[]="gmail.com";
    int j=strlen(email)-1;
    for(int i=strlen(data)-1;i>=0;i--)
    {
        if(data[i]!=email[j])
        {
            printf("Invaild");
            return 0;
        }
        j--;
    }
    printf("Vaild");
} 
Enter the email :klsf.@gmail.com
Invaild

// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>
int main() 
{
   char ch[]="abc";
   for(int i=0;i<strlen(ch);i++)
   {
       ch[i]=ch[i]-32;
   }
   printf("%s",ch);
   return 0;
}
ABC 

// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>
int main() 
{
   char ch[]="abDe4";
   for(int i=0;i<strlen(ch);i++)
   {
       if(ch[i]>=97&&ch[i]<=122)
       ch[i]=ch[i]-32;
       else if(ch[i]>=65&&ch[i]<=90)
       ch[i]=ch[i]+32;
    }
    printf("%s",ch);
    return 0;
}
ABdE4


// Online C compiler to run C program online
#include <stdio.h>
#include<string.h>
int main() 
{
    char ch[]="hello world";
    int count=0;
    for(int i=strlen(ch)-1;i>=0;i--)
    {
        if(ch[i]==' '&&count==0)
        continue;
        else if(ch[i]==' '&&count>0)
        break;
        else
        count++;
    }
    printf("%d",count);
    return 0;
}
5


// Online C compiler to run C program online
#include <stdio.h>
#include<string.h>
int main() 
{
    char ch[]="aabbbccddaa";
    char current=ch[0];
    int count=1;
    for(int i=1;i<=strlen(ch);i++)
    {
        if(current==ch[i])
        count++;
        else
       {
           printf("%c:%d\n",current,count);
           current=ch[i];
           count=1;
       }
    }
    return 0;
}
a:2
b:3
c:2
d:2
a:2