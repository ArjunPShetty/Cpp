// Online C compiler to run C program online
#include <stdio.h>
#include <math.h>

int main() 
{
    int n,count=0,sum=0,temp;
    printf("Enter number:");
    scanf("%d",&n);
    temp=n;
    while(temp>0)
    {
        count++;
        temp/=10;
    }
    temp=n;
    while(temp>0)
    {
        sum=sum+pow(temp%10,count);
        temp/=10;
    }
    if (sum==n)
    printf("%d is amstrong number",n);
    else
    printf("%d is not a amstrong number",n);
    return 0;
}
Enter number:153
153 is amstrong number


