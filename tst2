// Online C compiler to run C program online
#include <stdio.h>
#include <math.h>

int main() 
{
    int n,k,count=0,r=0;
    printf("Enter number:");
    scanf("%d",&n);
    printf("Enter position:");
    scanf("%d",&k);
    while(n>0)
    {
        r=n%2;
        count++;
        if(count==k)
        break;
        n=n/2;
    }
    if(r==1)
    printf("set");
    else
    printf("unset");
    
    return 0;
}
Enter number:21
Enter position:3
set

// Online C compiler to run C program online
#include <stdio.h>
#include <math.h>

int main() 
{
    int n,k;
    printf("Enter number:");
    scanf("%d",&n);
    printf("Enter position:");
    scanf("%d",&k);
    if(((n>>(k-1))&1)==1)
    printf("set");
    else
    printf("unset");
    
    return 0;
}
Enter number:21
Enter position:3
set

#include <stdio.h>
void divya(void);
void ramya(void);
int main() 
{
   printf("Darshan is in home\n");
   divya();
   printf("Darshan is back in his home");
}
void ramya()
{
    printf("He had tea with ramya\n");
}
void divya()
{
    printf("Divya is preparing coffee\n");
    ramya();
    printf("He had coffee with divya\n");
}
Darshan is in home
Divya is preparing coffee
He had tea with ramya
He had coffee with divya
Darshan is back in his home


// Online C compiler to run C program online
#include <stdio.h>
int reverse(int);
int main()
{
    int n;
    printf("Enter a number: ");
    scanf("%d",&n);
    int r=reverse(n);
    printf("%d",r);
}
int reverse(int n)
{
    int r=0;
    while(n>0)
    {
        r=r*10+n%10;
        n/=10;
    }
    return r;
}
Enter a number: 1234
4321

// Online C compiler to run C program online
#include <stdio.h>
void power(int);
int main() 
{
    int n; 
    printf("enter number:");
    scanf("%d",&n);
    power(n);
}
void power(int n)
{
    int flag=0;
    for(int i=1;i<=n;i=i*2)
    {
        if (i==n)
        flag=1;
    }
    if(flag==1)
    printf("%d is power of 2",n);
    else
    printf("%d is not power of 2",n);
}
enter number:16
16 is power of 2

