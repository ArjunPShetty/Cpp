// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    int n=7;
    for(int i=n;i>=1;i--)
    {
        for(int j=1;j<=i;j++)
        printf("*");
        for(int k=1;k<=(n-i);k++)
        printf(" ");
        for(int k=1;k<=(n-i);k++)
        printf(" ");
        for(int j=1;j<=i;j++)
        printf("*");
        printf("\n");
    }
    for(int i=2;i<=n;i++)
    {
        for(int j=1;j<=i;j++)
        printf("*");
        for(int k=1;k<=(n-i);k++)
        printf(" ");
        for(int k=1;k<=(n-i);k++)
        printf(" ");
        for(int j=1;j<=i;j++)
        printf("*");
        printf("\n");
    }
    return 0;
}
