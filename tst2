// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    // Write C code here
    int n;
    printf("enter the number of elements:");
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++)
    scanf("%d",&arr[i]);
    for(int i=1;i<n;i++)
    {
        if(arr[i]>arr[i-1])
        printf("%d ",arr[i]);
        else
        printf("%d ",arr[i-1]);
    }
    return 0;
}
enter the number of elements:6
3 6 2 8 10 7
6 6 8 10 10 


// Online C compiler to run C program online
#include <stdio.h>
int main() 
{
    // Write C code here
    int date=12;
    int cars[]={2375,7682,2325,2352};
    int fine[]={250,500,350,200};
    int n=sizeof(cars)/sizeof(cars[0]);
    int total_fine=0;
    for(int i=0;i<n;i++)
    {
        if(date%2==0&&cars[i]%2!=0)
        total_fine+=fine[i];
        else if(date%2!=0&&cars[i]%2==0)
        total_fine+=fine[i];
    }
    printf("Total fine =%d",total_fine);
    return 0;
}
Total fine =600

// Online C compiler to run C program online
#include <stdio.h>
int main() 
{
    // Write C code here
    int arr1[]={1,3,1,2};
    int arr2[]={2,2,3,1};
    int sum1=arr1[0],sum2=arr2[0];
    int n=sizeof(arr1)/sizeof(arr1[0]);
    for(int i=1;i<n;i++)
    {
        sum1+=i%2==0?arr1[i]:arr2[i];
        sum2+=i%2==0?arr2[i]:arr1[i];
    }
    sum1<sum2?printf("%d",sum1):printf("%d",sum2);
    return 0;
}
5

// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    // Write C code here
    int k=10,x,ans=0;
    int arr1[]={3,4,5};
    int arr2[]={4,4,5};
    int n=sizeof(arr1)/sizeof(arr1[0]);
    for (int i=0;i<n;i++)
    {
        x=(k/arr1[i])*arr2[i];
        if(x>ans)
        ans=x;
    }
    printf("%d",ans);
    return 0;
}
12

// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    // Write C code here
    int n=1004,sum=0;
    while(n>0)
    {
        int r=n%10;
        if(r==0)
        sum=sum*10+5;
        else
        sum=sum*10+r;
        n/=10;
    }
    while(sum>0)
    {
        n=n*10+sum%10;
        sum/=10;
    }
    printf("%d",n);
    return 0;
}
1554

// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    // Write C code here
    int n;
    printf("Enter number of elements:");
    scanf("%d",&n);
    int arr[n];
    printf("Enter the array elements:\n");
    for(int i=0;i<n;i++)
    scanf("%d",&arr[i]);
    int max=arr[0];
    for (int i=0;i<n;i++)
    {
        if(arr[i]>max)
        max=arr[i];
    }
    printf("Maximum number: %d",max);
    return 0;
}
Enter number of elements:5
Enter the array elements:
2 3 4 5 6
Maximum number: 6


