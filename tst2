// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    // Write C code here
    int n,x,i,t,h=0;
    printf("Enter number of elements:");
    scanf("%d",&n);
    int arr[n];
    printf("Enter the array elements:\n");
    for(int i=0;i<n;i++)
    scanf("%d",&arr[i]);
    printf("Enter the element:");
    scanf("%d",&x);
    for (i=0;i<n;i++)
    {
        if(x==arr[i])
        {
            h=1;
            printf("%d is in array at position:%d",x,i);
            break;
            
        }
    }
    if(!h)
    printf("%d is not in the array",x);
    return 0;
}
Enter number of elements:5
Enter the array elements:
3 4 5 2 1
Enter the element:3
3 is in array at position:0

=== Code Execution Successful ===

// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    // Write C code here
    int n,x,h,i;
    printf("Enter number of elements:");
    scanf("%d",&n);
    int arr[n];
    printf("Enter the array elements:\n");
    for(int i=0;i<n;i++)
    scanf("%d",&arr[i]);
    printf("Enter the element:");
    scanf("%d",&x);
    for (i=n-1;i>=0;i--)
    {
        if(x==arr[i])
        {
            h=1;
            printf("The number is at index: %d",i);
            break;
        }
    }
    if(!h)
    printf("%d is not in the array",x);
    return 0;
}
Enter number of elements:3
Enter the array elements:
3 4 3
Enter the element:3
The number is at index: 2

// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    // Write C code here
    int n,x,h,i,c=0;
    printf("Enter number of elements:");
    scanf("%d",&n);
    int arr[n];
    printf("Enter the array elements:\n");
    for(int i=0;i<n;i++)
    scanf("%d",&arr[i]);
    printf("Enter the element:");
    scanf("%d",&x);
    for (i=0;i<=n;i++)
    {
        if(x==arr[i])
        {
            c+=1;
        }
    }
    printf("%d is repeted %d times",x,c);
    return 0;
}
Enter number of elements:5
Enter the array elements:
1 2 1 4 1
Enter the element:1
1 is repeted 3 times



// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    int r,c;
    scanf("%d %d",&r,&c);
    int a[r][c];
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        scanf("%d",&a[i][j]);
    }
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        printf("%d\t",a[i][j]);
        printf("\n");
    }
    return 0;
}
2 3
1 2 3 4 5 6
1	2	3	
4	5	6


// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    int r,c;
    scanf("%d %d",&r,&c);
    int a[r][c];
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        scanf("%d",&a[i][j]);
    }
    int key;
    scanf("%d",&key);
    for(int i=0;i<r;i++)
    {
        for(int j=0;j<c;j++)
        if (key==a[i][j])
        printf("%d is at %d %d",key,i,j);
    }
    
    return 0;
}
2 3
1 2 3 4 5 6
4
4 is at 1 0


#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int a[n][n];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        scanf("%d",&a[i][j]);
        printf("\n");
    }
    int b[n][n];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            b[j][i]=a[i][j];
        }
    } 
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        printf("%d\t",b[i][j]);
        printf("\n");
    }
    return 0;
}
3
1 2 3
4 5 6
7 8 9
1	4	7	
2	5	8	
3	6	9

// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>

int main() 
{
    char names[4][10]={"Amogh","Anand","keerthan","Darshan"};
    int n=4;
    char data[10];
    scanf("%s",data);
    for(int i=0;i<n;i++)
    {
        if(strcmp(names[i],data)==0)
        {printf("Yes");return 0;}
    }
    printf("No");
}
