// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    int n=4;
    for(int i=n;i>=1;i--)
    {
        for(int j=1;j<=(n-i);j++)
        printf(" ");
        for(int k=1;k<=(2*i-1);k++)
        {
            printf("*");
        }
        printf("\n");
    }
    for(int i=2;i<=n;i++)
    {
        for(int j=1;j<=(n-i);j++)
        printf(" ");
        for(int k=1;k<=(2*i-1);k++)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
